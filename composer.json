{
    "name": "tjgazel/laravel-bs4-alert",
    "description": "Bootstrap 4 alerts for laravel 5.*",
    "keywords": ["laravel", "Boostrap 4", "Alerts", "Facade", "helper", "auto discover", "php"],
    "homepage": "https://github.com/tjgazel/laravel-bs4-alert",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Talles Gazel",
            "email": "tjgazel@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0.0",
        "illuminate/session": ">=5.0.0"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "TJGazel\\Bs4Alert\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "TJGazel\\Bs4Alert\\Bs4AlertServiceProvider"
            ],
            "aliases": {
                "Toastr": "TJGazel\\Bs4Alert\\Facades\\Bs4Alert"
            }
        }
    }
}
